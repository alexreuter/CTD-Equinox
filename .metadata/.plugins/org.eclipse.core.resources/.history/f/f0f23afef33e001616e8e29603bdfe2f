
public class FractionMain 
{
	 public static void main(String[] args)
	 {
	     Fraction f1, f2,f4;
	     
	     f1 = new Fraction(1, -2);
	     f2 = new Fraction(4, -8);
	     
	     
	     System.out.println(f1 + " - " + f2 + " = " + f1.subtract(f2));
	     
	     f4 = new Fraction(-8, -3);
	     f4.reciprocal();
	     f4.reciprocal();
	     System.out.println(f4);
	    
	 }
}
