import acm.program.*;
import acm.graphics.*;
import java.awt.Color;


public class MiddlePaddle extends GRect
{
	public MiddlePaddle(double x, double y, double width, double height, Color c)
	{
		super(x,y,width,height);
		
		setColor(c);
        setFillColor(c);
        setFilled(true);
        
	}
}
