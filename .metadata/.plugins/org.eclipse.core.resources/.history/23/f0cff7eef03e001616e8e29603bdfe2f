
public class FractionMain 
{
	 public static void main(String[] args)
	 {
	     Fraction f1, f2, f3,f4;
	     
	     f1 = new Fraction(2, 7);
	     f2 = new Fraction(4, 24);
	     f3 = f1.divide(f2);
	     
	     System.out.println(Fraction.times(f1, f2));
	     
	     f4 = new Fraction(8, 3);
	     f4.reciprocal();
	     System.out.println(f4);
	    
	 }
}
